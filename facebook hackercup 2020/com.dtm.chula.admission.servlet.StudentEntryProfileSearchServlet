document.ProfileForm.IdCard.focus();
if (document.ProfileForm.Nation.value != "01") {
	document.all("cardlabel").innerText = 'เลขที่หนังสือเดินทาง';
    if 	(document.ProfileForm.IDay.value == "00")
	document.ProfileForm.IDay.value = "";
    if 	(document.ProfileForm.IMon.value == "00")
	document.ProfileForm.IMon.value = "";
	if 	(document.ProfileForm.EDay.value == "00")
	document.ProfileForm.EDay.value = "";
    if 	(document.ProfileForm.EMon.value == "00")
	document.ProfileForm.EMon.value = "";
}
if 	(document.ProfileForm.BDay.value == "00")
	document.ProfileForm.BDay.value = "";
if 	(document.ProfileForm.BMon.value == "00")
	document.ProfileForm.BMon.value = "";
