<script type='text/javascript' language="JavaScript1.2">
	var status = "";
   
   
   
   
    function verifyAttribuate()
    {
	  
	    
	   if  (document.ProfileForm.submitType.value == 'U') 
	     {   
	     
	     if  (document.ProfileForm.IdCard.value.length < 13 && document.ProfileForm.Nation.value == '01') 
	     {
	     	window.alert(" กรุณาบันทึกเลขประจำตัวประชาชน ");
	     	document.ProfileForm.IdCard.focus()
	     	return false;
	     }
	     if  (document.ProfileForm.IdCard.value.length < 1) 
	     {
	     	window.alert(" กรุณาบันทึกเลขที่หนังสือเิดินทาง ");
	     	document.ProfileForm.IdCard.focus()
	     	return false;
	     }
	     if  (document.ProfileForm.BYear.value.length < 4 ) 
	     {
	     	window.alert(" กรุณาบันทึกปีให้ถูกต้อง ");
	     	document.ProfileForm.BYear.focus()
	     	return false;
	     }
	     
	     if  (document.ProfileForm.Nation.value != '01' && document.ProfileForm.IYear.value.length < 4 ) 
	     {
	     	window.alert(" กรุณาบันทึกปีให้ถูกต้อง ");
	     	document.ProfileForm.IYear.focus()
	     	return false;
	     }
	     
	     if  (document.ProfileForm.Nation.value != '01' && document.ProfileForm.EYear.value.length < 4 ) 
	     {
	     	window.alert(" กรุณาบันทึกปีให้ถูกต้อง ");
	     	document.ProfileForm.EYear.focus()
	     	return false;
	     }
	     
	     if  (document.ProfileForm.Nation.value != '01' && !checkIDate()) 
	     {
	     	window.alert(" กรุณาวันที่ให้ถูกต้อง ");
	     	document.ProfileForm.IDay.focus()
	     	return false;
	     }
	     
	     if  (document.ProfileForm.Nation.value != '01' && !checkEDate()) 
	     {
	     	window.alert(" กรุณาวันที่ให้ถูกต้อง ");
	     	document.ProfileForm.EDay.focus()
	     	return false;
	     }
	     
	     if  (document.ProfileForm.Nation.value != '01' && !checkIEDate()) 
	     {
	     	window.alert(" กรุณาวันที่ให้ถูกต้อง ");
	     	document.ProfileForm.EDay.focus()
	     	return false;
	     }
	     
	    	     
	      if  (document.ProfileForm.MobilePhoneNo.value.length < 10 && document.ProfileForm.Nation.value == '01') 
	     {
	     	window.alert(" กรุณาบันทึกเบอร์โทรศัพท์มือถือให้ถูกต้อง ");
	     	document.ProfileForm.MobilePhoneNo.focus()
	     	return false;
	     }
	     
	     if  (!checkValidDate()) 
	     {
	     	window.alert(" กรุณาวันที่ให้ถูกต้อง ");
	     	document.ProfileForm.BDay.focus()
	     	return false;
	     }
	     
	     if  (document.ProfileForm.FatherName.value.length < 1) 
	     {
	     	window.alert(" กรุณาบันทึกชื่อบิดา ");
	     	document.ProfileForm.FatherName.focus()
	     	return false;
	     }
	     
	      if  (document.ProfileForm.FatherSurName.value.length < 1) 
	     {
	     	window.alert(" กรุณาบันทึกนามสกุลบิดา ");
	     	document.ProfileForm.FatherSurName.focus()
	     	return false;
	     }
	     
	      if  (document.ProfileForm.Nation.value == '01' && document.ProfileForm.IdCardFather.value.length < 13  ) 
	     {
	     	window.alert(" กรุณาบันทึกเลขประจำตัวประชาชนบิดา ");
	     	document.ProfileForm.IdCardFather.focus()
	     	return false;
	     }
	    
	     if  (document.ProfileForm.ProfileStatus.value != 'S' && document.ProfileForm.MatherName.value.length < 1) 
	     {
	     	window.alert(" กรุณาบันทึกชื่อมารดา ");
	     	document.ProfileForm.MatherName.focus()
	     	return false;
	     }
	     if  (document.ProfileForm.ProfileStatus.value != 'S' && document.ProfileForm.MatherSurName.value.length < 1) 
	     {
	     	window.alert(" กรุณาบันทึกนามสกุลมารดา ");
	     	document.ProfileForm.MatherSurName.focus()
	     	return false;
	     } 
	      if  (document.ProfileForm.Nation.value == '01' && document.ProfileForm.IdCardMother.value.length < 13 ) 
	     {
	     	window.alert(" กรุณาบันทึกเลขประจำตัวประชาชนมารดา ");
	     	document.ProfileForm.IdCardMother.focus()
	     	return false;
	     }
	     
         if  (document.ProfileForm.AdmType.value == 'B') 
	     {
	     	/*guardianCheck();*/
	     	guarantorCheck();
	     	guardianRelationCheck();
	     	if  (document.ProfileForm.GuardianName.value.length < 1) 
		       {
		     	window.alert(" กรุณาบันทึกชื่อผู้ปกครอง ");
		     	document.ProfileForm.GuardianName.focus()
		     	return false;
		     }
		      if  (document.ProfileForm.GuardianSurName.value.length < 1) 
		     {
		     	window.alert(" กรุณาบันทึกนามสกุลผู้ปกครอง ");
		     	document.ProfileForm.GuardianSurName.focus()
		     	return false;
		     }
		      if  (document.ProfileForm.Nation.value == '01' && document.ProfileForm.IdCardGuardian.value.length < 13) 
	        {
	     	    window.alert(" กรุณาบันทึกเลขประจำตัวประชาชนผู้ปกครอง ");
	     	    document.ProfileForm.IdCardGuardianr.focus()
	     	    return false;
	        }
		     
		     
		     if  (document.ProfileForm.GuarantorName.value.length < 1) 
		       {
		     	window.alert(" กรุณาบันทึกชื่อผู้ค้ำประกัน ");
		     	document.ProfileForm.GuarantorName.focus()
		     	return false;
		     }
		      if  (document.ProfileForm.GuarantorSurName.value.length < 1) 
		     {
		     	window.alert(" กรุณาบันทึกนามสกุลผู้ค้ำประกัน ");
		     	document.ProfileForm.GuarantorSurName.focus()
		     	return false;
		     }
		      
	      }  
	      
	     }   
	     
		document.ProfileForm.Province.disabled=false;
		
	    return true;
    }
    
    function setPrint()
    { 
      document.ProfileForm.printStatus.value = "Y";
      document.ProfileForm.action = "/servlet/com.dtm.chula.admission.servlet.StudentEntryProfileSearchServlet" ;
      return true;
    
    }
		
    
    function checkValidDate()
    {   
    	var iyear = document.ProfileForm.BYear.value;
    	var imon = document.ProfileForm.BMon.value;
    	var iday  = document.ProfileForm.BDay.value;
    	if (isblank(imon)) 
    	   return true;
    	else if (imon.length < 2) 
    	   imon = '0'+imon;	 	
    	if (isblank(iday)) 
    	   return true;
    	else if (iday.length < 2)
    	   iday = '0'+iday;	
       if (iyear > 2400) 
	  	  iyear = iyear - 543;
       var test = new Date(iyear,imon-1,iday);
       if (iyear == y2k(test.getYear()) && 
          (imon-1 == test.getMonth()) && 
          (iday == test.getDate())) 
       {
         return true;
       } else {
         return false;
       }
    }
    
    function checkIDate()
    {   
    	var iyear = document.ProfileForm.IYear.value;
    	var imon = document.ProfileForm.IMon.value;
    	var iday  = document.ProfileForm.IDay.value;
    	if (isblank(imon)) 
    	   return true;
    	else if (imon.length < 2) 
    	   imon = '0'+imon;	 	
    	if (isblank(iday)) 
    	   return true;
    	else if (iday.length < 2)
    	   iday = '0'+iday;	
       if (iyear > 2400) 
	  	  iyear = iyear - 543;
       var test = new Date(iyear,imon-1,iday);
       if (iyear == y2k(test.getYear()) && 
          (imon-1 == test.getMonth()) && 
          (iday == test.getDate())) 
       {
         return true;
       } else {
         return false;
       }
    }
    
     function checkEDate()
    {   
    	var iyear = document.ProfileForm.EYear.value;
    	var imon = document.ProfileForm.EMon.value;
    	var iday  = document.ProfileForm.EDay.value;
    	if (isblank(imon)) 
    	   return true;
    	else if (imon.length < 2) 
    	   imon = '0'+imon;	 	
    	if (isblank(iday)) 
    	   return true;
    	else if (iday.length < 2)
    	   iday = '0'+iday;	
       if (iyear > 2400) 
	  	  iyear = iyear - 543;
       var test = new Date(iyear,imon-1,iday);
       if (iyear == y2k(test.getYear()) && 
          (imon-1 == test.getMonth()) && 
          (iday == test.getDate())) 
       {
         return true;
       } else {
         return false;
       }
    }
    
      function checkIEDate()
    {   
    	var iyear = document.ProfileForm.IYear.value;
    	var imon = document.ProfileForm.IMon.value;
    	var iday  = document.ProfileForm.IDay.value;
    	if (imon.length < 2)   imon = '0'+imon;
    	if (iday.length < 2)   iday = '0'+iday;	
    	var idate = iyear.concat(imon) ;
    	idate = idate.concat(iday) ;
    	var eyear = document.ProfileForm.EYear.value;
    	var emon = document.ProfileForm.EMon.value;
    	var eday  = document.ProfileForm.EDay.value;
    	if (emon.length < 2)   emon = '0'+emon;
    	if (eday.length < 2)   eday = '0'+eday;	
    	var edate = eyear.concat(emon) ;
    	edate = edate.concat(eday) ;
    	if (idate >= edate){
    	    return false;
        } else {
            return true;
        }
    }
    
    function validCountry() 
    {
    	if 	(document.ProfileForm.Country.value != "01") 
    	{
    		document.ProfileForm.Province.value = "99";
			document.ProfileForm.Province.disabled=true;
			
    	} else {
			document.ProfileForm.Province.disabled=false;
			if 	(document.ProfileForm.Province.value == "") 
    	     {document.ProfileForm.Province.value = "10";}
		}
    }
    
    function validProvince() 
    {
    	if 	(document.ProfileForm.Province.value == "99" &&
    	     document.ProfileForm.Country.value == "01") 
    	{
	     	window.alert(" กรุณาบันทึกจังหวัดให้ถูกต้อง ");
    		document.ProfileForm.Province.value = "10";
    		document.ProfileForm.Province.focus();
	    } 	
    }
    
    function guardianRelationCheck() 
    {
    	if 	(document.ProfileForm.GuardianRelation.value == "1") 
    	{
    	   document.ProfileForm.GuardianName.value = document.ProfileForm.FatherName.value ;
           document.ProfileForm.GuardianSurName.value = document.ProfileForm.FatherSurName.value ;
           if  (document.ProfileForm.Nation.value == '01') {
                document.ProfileForm.IdCardGuardian.value = document.ProfileForm.IdCardFather.value ; 
           }
           document.ProfileForm.GuardianName.focus();
	     	
	    } else	if 	(document.ProfileForm.GuardianRelation.value == "2") 
    	{
	       document.ProfileForm.GuardianName.value = document.ProfileForm.MatherName.value ;
           document.ProfileForm.GuardianSurName.value = document.ProfileForm.MatherSurName.value ;
           if  (document.ProfileForm.Nation.value == '01') {
                document.ProfileForm.IdCardGuardian.value = document.ProfileForm.IdCardMother.value ; 
           }
           document.ProfileForm.GuardianName.focus();
	    } 
    }
    
     function otherCheck()
    {
       if 	(document.ProfileForm.Guardian[2].checked) 
             {
              document.ProfileForm.GuardianName.value = ""; 
              document.ProfileForm.GuardianSurName.value = ""; 
              document.ProfileForm.GuardianName.focus();
             }
             
       if 	(document.ProfileForm.Guarantor[3].checked)
            {
              document.ProfileForm.GuarantorName.value = "";
              document.ProfileForm.GuarantorSurName.value = "";
              document.ProfileForm.GuarantorName.focus();
            }
       
    }
    
    function guardianCheck()
    {
       if 	(document.ProfileForm.Guardian[0].checked) 
            {
              document.ProfileForm.GuardianName.value = document.ProfileForm.FatherName.value ;
              document.ProfileForm.GuardianSurName.value = document.ProfileForm.FatherSurName.value ;
            }
       else if 	(document.ProfileForm.Guardian[1].checked) 
            {
              document.ProfileForm.GuardianName.value = document.ProfileForm.MatherName.value ;
              document.ProfileForm.GuardianSurName.value = document.ProfileForm.MatherSurName.value ;
            }
      
    }
    
    function guarantorCheck()
    {
       if 	(document.ProfileForm.Guarantor[0].checked) 
            {
              document.ProfileForm.GuarantorName.value = document.ProfileForm.FatherName.value ;
              document.ProfileForm.GuarantorSurName.value = document.ProfileForm.FatherSurName.value ;
            }
       else if 	(document.ProfileForm.Guarantor[1].checked) 
            {
              document.ProfileForm.GuarantorName.value = document.ProfileForm.MatherName.value ;
              document.ProfileForm.GuarantorSurName.value = document.ProfileForm.MatherSurName.value ;
            }
       else if 	(document.ProfileForm.Guarantor[2].checked) 
            {
              document.ProfileForm.GuarantorName.value = document.ProfileForm.GuardianName.value ;
              document.ProfileForm.GuarantorSurName.value = document.ProfileForm.GuardianSurName.value ;
            }
      
    }
</script>
